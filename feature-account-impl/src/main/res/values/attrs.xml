<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="pinCodeButtonTextColor" format="reference" />
    <attr name="pinCodeButtonBackground" format="reference" />
    <attr name="pinCodeEmptyDotStrokeColor" format="reference" />
    <attr name="pinCodeEmptyDotStrokeWidth" format="reference" />
    <attr name="pinCodeButtonTextSize" format="reference" />

    <declare-styleable name="MnemonicWordView">
        <attr name="wordText" format="string" />
        <attr format="enum" name="colorMode">
            <enum name="light" value="0" />
            <enum name="dark" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SettingsItemView">
        <attr name="title" />
        <attr name="settingValue" format="string" />
        <attr name="icon" />
    </declare-styleable>

    <declare-styleable name="MnemonicContainerView">

        <attr name="showBackground" format="boolean" />
    </declare-styleable>
</resources>